{
  "name": "qui-browser-vr",
  "version": "2.0.0",
  "description": "Qui Browser VR v2.0.0 - Production-ready VR browser with 17 features (Tier 1-3), complete CI/CD automation, and enterprise monitoring. Optimized for Meta Quest 2/3 with 90-120 FPS, Japanese IME, hand tracking, spatial audio, WebGPU, multiplayer, AI recommendations, and more.",
  "main": "dist/index.html",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.js'",
    "test:tier": "jest tests/tier-system-integration.test.js",
    "test:integration": "jest --testMatch='**/tests/*integration*.test.js'",
    "test:e2e": "playwright test",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,yml,yaml}\"",
    "benchmark": "node tools/benchmark.js",
    "benchmark:all": "node tools/benchmark.js --all --iterations 500 --output benchmark-results.json --format json",
    "benchmark:report": "node tools/benchmark.js --all --output benchmark-results.md --format markdown",
    "benchmark:regression": "node tools/check-performance-regression.js benchmark-results.json",
    "ci:lint": "npm run lint && npm run format:check",
    "ci:test": "npm run test:coverage",
    "ci:benchmark": "npm run benchmark:all && npm run benchmark:regression",
    "ci:all": "npm run ci:lint && npm run ci:test && npm run ci:benchmark",
    "docker:build": "docker build -t qui-browser-vr:2.0.0 .",
    "docker:run": "docker run -d -p 8080:80 --name qui-browser qui-browser-vr:2.0.0",
    "docker:stop": "docker stop qui-browser && docker rm qui-browser",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:logs": "docker logs -f qui-browser",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "clean": "rm -rf dist node_modules coverage .cache",
    "clean:install": "npm run clean && npm install",
    "prepare": "husky install || true"
  },
  "keywords": [
    "vr",
    "webxr",
    "virtual-reality",
    "meta-quest",
    "pico",
    "browser",
    "lightweight",
    "3d",
    "spatial-audio",
    "hand-tracking"
  ],
  "dependencies": {
    "three": "^0.152.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "stripe": "^14.10.0",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "terser-webpack-plugin": "^5.3.9",
    "compression-webpack-plugin": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "core-js": "^3.31.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "typescript": "^5.1.6",
    "ts-loader": "^9.4.4"
  },
  "author": "Qui Browser Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
