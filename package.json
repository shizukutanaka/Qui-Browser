{
  "name": "qui-browser-vr",
  "version": "2.0.0",
  "description": "Qui Browser VR - Lightweight WebXR browser for VR devices",
  "main": "index.html",
  "private": true,
  "scripts": {
    "start": "echo 'Open index.html in your VR device browser'",
    "serve": "python -m http.server 8000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t qui-browser-vr:2.0.0 .",
    "docker:run": "docker run -d -p 8080:80 qui-browser-vr:2.0.0",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "vr",
    "webxr",
    "virtual-reality",
    "meta-quest",
    "pico",
    "browser",
    "lightweight",
    "3d",
    "spatial-audio",
    "hand-tracking"
  ],
  "dependencies": {
    "three": "^0.152.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "author": "Qui Browser Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
