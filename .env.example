# Qui Browser VR - Environment Configuration
# Copy this file to .env and update with your values

# Node Environment
NODE_ENV=development

# Application
VR_BROWSER_VERSION=5.7.0
VR_BROWSER_NAME="Qui Browser VR"

# Performance Targets
VR_DEFAULT_FPS_TARGET=90
VR_MIN_FPS_TARGET=72
VR_FRAME_TIME_BUDGET=11.1

# Memory Management
VR_MEMORY_LIMIT_MB=2048
VR_MEMORY_WARNING_MB=1800
VR_MEMORY_CRITICAL_MB=1950

# Default Settings
DEFAULT_ENVIRONMENT=space
DEFAULT_UI_LAYOUT=comfortable
DEFAULT_HAND_TRACKING=true
DEFAULT_EYE_TRACKING=true
DEFAULT_FOVEATED_RENDERING=auto

# Logging
DEBUG_MODE=false
LOG_LEVEL=info

# Features
ENABLE_ML_GESTURES=true
ENABLE_SPATIAL_ANCHORS=true
ENABLE_NEURAL_RENDERING=true
ENABLE_EYE_TRACKING=true
ENABLE_BODY_IK=true

# Performance
ENABLE_FOVEATED_RENDERING=true
ENABLE_OBJECT_POOLING=true
ENABLE_MEMORY_OPTIMIZATION=true
ENABLE_PERFORMANCE_MONITORING=true
